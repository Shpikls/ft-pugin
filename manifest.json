{
  "manifest_version": 3,
  "name": "ESMP Feature Switcher",
  "version": "1.0.0",
  "description": "Manage feature flags for ESMP frontend application",
  "permissions": [
    "activeTab",
    "scripting",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/disabled/icon16.png",
      "48": "icons/disabled/icon48.png",
      "128": "icons/disabled/icon128.png"
    },
    "default_title": "ESMP Feature Switcher"
  },
  "icons": {
    "16": "icons/disabled/icon16.png",
    "48": "icons/disabled/icon48.png",
    "128": "icons/disabled/icon128.png"
  }
}
